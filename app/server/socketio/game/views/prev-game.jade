#prev-game.modal.fade(style='overflow:auto')
	.modal-dialog
		.modal-content
			.modal-header.text-center
				a.close(data-dismiss='modal') &times;
				h3 Previous game
			.modal-body.text-center
				.div(ng-repeat='round in tableData.prevGame track by $index')
					ul.position-set.card-set.clearfix.list-unstyled.list-inline(ng-cloak)
						li.card-holder.card-position(ng-repeat='obj in round track by $index')
							.card.green-border(ng-class="{'red-border': obj.player%2 == 1}" style='background-position:{{obj.card.bgPos[0]}}px {{obj.card.bgPos[1]}}px')
							span.player-name(ng-class="{'color-metroRed': obj.player%2 == 1}" ng-bind='tableData.players[obj.player]')
