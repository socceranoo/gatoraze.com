
extends layout

block content
	.row
		h2.text-center Members
		.col-md-10.col-md-offset-1.transaction-table.background-opacity
			table.table
				thead
					tr
						th(style='width:40px') #
						th(style='width:180px') Name
						th(style='width:200px') Location
						th(style='width:180px') Username
						th Account Created
				tbody
				- for (var i = 0; i < accts.length; i++)
					tr
						td= i+1
						td= accts[i].name
						td= accts[i].country
						td= accts[i].user
						td= accts[i].date
